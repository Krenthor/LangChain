        "\n",
        "\n",
        "prompt = PromptTemplate(input_variables=[\"human_input\"], template=template)\n",
        "\n",
        "\n",
        "chatgpt_chain = LLMChain(\n",
        "    llm = OpenAI(temperature=.8,model_name=\"gpt-4\"),,\n",
        "    prompt=prompt,\n",
        "    verbose=True,\n",
        "    memory=ConversationBufferWindowMemory(k=2),\n",
        ")\n",
        "\n",
        "\n",
        "\n",
        "\n"
